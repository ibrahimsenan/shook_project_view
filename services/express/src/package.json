{
  "name": "shooka-project",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "test": "set NODE_ENV=test&& jest",
    "start": "nodemon -L --legacy-watch --ignore ./services/mobile",
    "start-prod": "set NODE_ENV=production&& nodemon -L ./server.js",
    "lint": "run-s lint:*",
    "lint:code": "eslint --fix . --ext .js,.jsx,.ts,.tsx",
    "lint:type": "tsc",
    "lint:style": "stylelint --fix \"**/*.{css,scss,sass,ts,tsx}\"",
    "lint:format": "prettier -w . -u --loglevel silent",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{css,scss,sass}": "stylelint --fix"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "@root/(.*)": "<rootDir>/$1",
      "@routes/(.*)": "<rootDir>/src/routes/$1",
      "@redis/(.*)": "<rootDir>/src/redis/$1",
      "@modules/(.*)": "<rootDir>/src/modules/$1",
      "@api/(.*)": "<rootDir>/src/api/$1",
      "@utils/(.*)": "<rootDir>/src/api/utils/$1",
      "@auth/(.*)": "<rootDir>/src/api/auth/$1"
    },
    "modulePaths": [
      "<rootDir>"
    ]
  },
  "_moduleAliases": {
    "@root": ".",
    "@deep": "./src/api/auth/auth_service",
    "@routes": "./src/routes",
    "@modules": "./src/modules",
    "@api": "./src/api",
    "@utils": "./src/api/utils",
    "@auth": "./src/api/auth"
  },
  "_moduleDirectories": [
    "node_modules_custom"
  ],
  "keywords": [
    "Node",
    "Restaurant",
    "Backend",
    "Js",
    "Server",
    "Application"
  ],
  "author": "Ibrahim Senan",
  "license": "ISC",
  "dependencies": {
    "@cucumber/cucumber": "^8.6.0",
    "@redis/client": "^1.5.8",
    "@types/bcrypt": "^5.0.0",
    "@types/hat": "^0.0.1",
    "@types/joi": "^17.2.3",
    "@types/lodash": "^4.14.195",
